#NoEnv
#SingleInstance, Force
SendMode, Input
SetBatchLines, -1
SetWorkingDir, %A_ScriptDir%

; Base64 Encoded Images for Each Slot
; Slot 1
Base64PNG_Slot1_on := "" ; LS on
Base64PNG_Slot1_off := "" ; LS off

; Slot 2
Base64PNG_Slot2_on := "" ;  on
Base64PNG_Slot2_off := "your_base64_string_for_slot2_off" ;  off

; Slot 3
Base64PNG_Slot3_on := "your_base64_string_for_slot3_on"
Base64PNG_Slot3_off := "your_base64_string_for_slot3_off"

; Slot 4
Base64PNG_Slot4_on := "your_base64_string_for_slot4_on"
Base64PNG_Slot4_off := "your_base64_string_for_slot4_off"

; Slot 5
Base64PNG_Slot5_on := "your_base64_string_for_slot5_on"
Base64PNG_Slot5_off := "your_base64_string_for_slot5_off"

; Slot 6
Base64PNG_Slot6_on := "your_base64_string_for_slot6_on"
Base64PNG_Slot6_off := "your_base64_string_for_slot6_off"

; Example: Setting the tray icon for Slot 1 when it's active
Menu Tray, Icon, % "HBITMAP:*" hICON_Slot1 := CreateBitMap(Base64PNG_Slot1_on)

; Toggle states and corresponding actions for each slot
Toggle := false
^a:: ; Toggle the script on/off
Toggle := !Toggle
SetTrayIcons()
return

SetTrayIcons() {
    global Toggle

    if (Toggle) {
        Menu Tray, Icon, % "HBITMAP:*" hICON_Slot1 := CreateBitMap(Base64PNG_Slot1_on)
        Menu Tray, Icon, % "HBITMAP:*" hICON_Slot2 := CreateBitMap(Base64PNG_Slot2_on)
        Menu Tray, Icon, % "HBITMAP:*" hICON_Slot3 := CreateBitMap(Base64PNG_Slot3_on)
        Menu Tray, Icon, % "HBITMAP:*" hICON_Slot4 := CreateBitMap(Base64PNG_Slot4_on)
        Menu Tray, Icon, % "HBITMAP:*" hICON_Slot5 := CreateBitMap(Base64PNG_Slot5_on)
        Menu Tray, Icon, % "HBITMAP:*" hICON_Slot6 := CreateBitMap(Base64PNG_Slot6_on)
    } else {
        Menu Tray, Icon, % "HBITMAP:*" hICON_Slot1 := CreateBitMap(Base64PNG_Slot1_off)
        Menu Tray, Icon, % "HBITMAP:*" hICON_Slot2 := CreateBitMap(Base64PNG_Slot2_off)
        Menu Tray, Icon, % "HBITMAP:*" hICON_Slot3 := CreateBitMap(Base64PNG_Slot3_off)
        Menu Tray, Icon, % "HBITMAP:*" hICON_Slot4 := CreateBitMap(Base64PNG_Slot4_off)
        Menu Tray, Icon, % "HBITMAP:*" hICON_Slot5 := CreateBitMap(Base64PNG_Slot5_off)
        Menu Tray, Icon, % "HBITMAP:*" hICON_Slot6 := CreateBitMap(Base64PNG_Slot6_off)
    }
}

; Function to create a bitmap from a base64 string
CreateBitMap(ByRef strB64) {
    VarSetCapacity(B64, StrLen(strB64) << !!A_IsUnicode) ; Not needed on short string
    B64 := strB64, strB64 := "" ; free memory
    If !DllCall("Crypt32.dll\CryptStringToBinary", "Ptr", &B64, "UInt", 0, "UInt", 0x01, "Ptr", 0, "UIntP", DecLen, "Ptr", 0, "Ptr", 0)
        Return False
    VarSetCapacity(Dec, DecLen, 0)
    If !DllCall("Crypt32.dll\CryptStringToBinary", "Ptr", &B64, "UInt", 0, "UInt", 0x01, "Ptr", &Dec, "UIntP", DecLen, "Ptr", 0, "Ptr", 0)
        Return False
    hData := DllCall("Kernel32.dll\GlobalAlloc", "UInt", 2, "UPtr", DecLen, "UPtr")
    pData := DllCall("Kernel32.dll\GlobalLock", "Ptr", hData, "UPtr")
    DllCall("Kernel32.dll\RtlMoveMemory", "Ptr", pData, "Ptr", &Dec, "UPtr", DecLen)
    DllCall("Kernel32.dll\GlobalUnlock", "Ptr", hData)
    DllCall("Ole32.dll\CreateStreamOnHGlobal", "Ptr", hData, "Int", True, "PtrP", pStream)
    hGdip := DllCall("Kernel32.dll\LoadLibrary", "Str", "Gdiplus.dll", "UPtr")
    VarSetCapacity(SI, 16, 0), NumPut(1, SI, 0, "UChar")
    DllCall("Gdiplus.dll\GdiplusStartup", "PtrP", pToken, "Ptr", &SI, "Ptr", 0)
    DllCall("Gdiplus.dll\GdipCreateBitmapFromStream",  "Ptr", pStream, "PtrP", pBitmap)
    DllCall("Gdiplus.dll\GdipCreateHBITMAPFromBitmap", "Ptr", pBitmap, "PtrP", hBitmap, "UInt", 0)
    DllCall("Gdiplus.dll\GdipDisposeImage", "Ptr", pBitmap)
    DllCall("Gdiplus.dll\GdiplusShutdown", "Ptr", pToken)
    DllCall("Kernel32.dll\FreeLibrary", "Ptr", hGdip)
    DllCall(NumGet(NumGet(pStream + 0, 0, "UPtr") + (A_PtrSize * 2), 0, "UPtr"), "Ptr", pStream)
    Return hBitmap
}
